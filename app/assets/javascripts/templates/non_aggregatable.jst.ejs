<div class="row-fluid">
  <div class="span12">
    <hr>
    <br />
    <div class="col-sm-offset-1">
    <div class="result non-aggregatable" style="color: <%- fColor %>; font-family: <%- fFamily %>">
      <h4><%- resultObj.question_text %></h4>
      <ul>
        <% $.each(resultObj.results, function(index, answer) { %>
          <li><%- answer %></li>
        <% }); %>
      </ul>
    </div>
    </div>
    <br />
  </div>
</div>
<div class="clearfix"></div>
